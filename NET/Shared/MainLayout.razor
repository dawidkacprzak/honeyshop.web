@code{
    private readonly CancellationTokenSource TokenSource =
    new CancellationTokenSource();
    private bool sessionErrored = false;
    private bool initialized = false;
    private string detailedException = "";

    public void Dispose()
    {
        TokenSource.Cancel();
    }

    protected override async Task OnInitializedAsync()
    {
        await InitializeSession();
    }

    private async Task InitializeSession()
    {
        int retryCount = 0;
        bool finishedWithError = true;
        do
        {
            try
            {
                if (TokenSource.Token.IsCancellationRequested)
                {
                    return;
                }
                await uss.InitializeSession(TokenSource.Token).ConfigureAwait(false);
                finishedWithError = false;
                break;
            }
            catch (Exception ex)
            {
                detailedException = JsonConvert.SerializeObject(ex);
                if (TokenSource.Token.IsCancellationRequested)
                {
                    return;
                }
            }
            finally
            {
                retryCount++;
            }
        } while (finishedWithError && retryCount < 3);
        initialized = true;
        if (finishedWithError)
            sessionErrored = true;
    }
}